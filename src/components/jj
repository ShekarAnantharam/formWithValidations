    </td>
              <td className="table-data">
                {/* Insert age field */}
              </td>
              <td className="table-data">
                {/* Insert date of birth field */}
              </td>
              <td className="table-data">
                {/* Insert phone number field */}
              </td>
              <td className="table-data">
                {/* Insert addresses field */}
              </td>
              <td className="table-data">
                {/* Insert password field */}
              </td>
              <td className="table-data">
                {editingUserId === user.id ? (
                  <>
                    <button className="save-button" onClick={() => handleSave(user.id, user)}>Save</button>
                    <button className="cancel-button" onClick={() => setEditingUserId(null)}>Cancel</button>
                  </>
                ) : (
                  <>
                    <button className="edit-button" onClick={() => handleEdit(user.id)}>Edit</button>
                    <button className="delete-button" onClick={() => handleDelete(user.id)}>Delete</button>
                  </>
                )}
              </td>
            </tr>
          ))}
        </tbody>
      </table>
    </div>
  );
};

export default UserTable;